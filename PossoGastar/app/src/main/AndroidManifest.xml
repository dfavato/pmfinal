<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gabrielcardoso.possogastar">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManageAccounts" />
        <activity android:name=".BankStatement" />
        <activity android:name=".TransferMoney" />
        <activity android:name=".AccountItemDetailed" />
        <activity
            android:name=".NewRealAccount"
            android:label="@string/title_activity_new_real_account"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NewAccountingAccount"
            android:label="@string/title_activity_new_account_account"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>